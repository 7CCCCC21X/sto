{
  "version": 2,
  "builds": [
    {
      "src": "api/check-stake.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/check-stake", "dest": "api/check-stake.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
